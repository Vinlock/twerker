{
  "name": "twerker",
  "version": "2.0.1",
  "description": "TypeScript Worker Threads Made Simple - Run TS functions in worker threads with type safety",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/example.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
    "example:worker-file": "node dist/examples/use-worker-file.js",
    "example:enrichment": "node dist/examples/run-enrichment-service.js",
    "example:simple": "node dist/example-simple.js",
    "example:fibonacci": "ts-node examples/simple-example.ts",
    "demo": "ts-node src/demo.ts",
    "simple-demo": "ts-node src/simple-demo.ts",
    "performance": "ts-node src/performance-demo.ts",
    "exit-test": "ts-node src/exit-test.ts",
    "poolifier-test": "ts-node src/poolifier-test.ts"
  },
  "keywords": [
    "typescript",
    "worker",
    "thread",
    "multithreading",
    "parallel",
    "worker-threads",
    "pool",
    "threadpool"
  ],
  "author": "Dak Washbrook",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dakdevs/twerker.git"
  },
  "bugs": {
    "url": "https://github.com/dakdevs/twerker/issues"
  },
  "homepage": "https://github.com/dakdevs/twerker#readme",
  "license": "ISC",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.17.19",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}